{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "www",
    "site": "alumnimatch-f290c",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "appAssociation": "NONE",
    "headers": [
      {
        "source": "/.well-known/apple-app-site-association",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/pkcs7-mime"
          }
        ]
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
